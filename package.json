  "homepage": "https://vuemadness.com/nuxt-chatgpt",
  "bugs": {
    "url": "https://github.com/schnapsterdog/nuxt-chatgpt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schnapsterdog/nuxt-chatgpt"
  },
  "contributors": [
    {
      "name": "Oliver Trajceski (@schnapsterdog)"
    }
  ],
